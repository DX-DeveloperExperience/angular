{
  "name": "application",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "run-s build:default build:fr",
    "build:default": "ng build --prod --baseHref=/en/",
    "build:fr": "ng build --prod --baseHref=/fr/ --i18nFile=src/locale/messages.fr.xlf --i18nLocale=fr --outputPath=dist/Application/fr",
    "test": "ng test",
    "test:ci": "ng test --watch=false --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.{css,ts}\"",
    "webpack-report": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats.json",
    "prettier-test": "prettier -l 'src/**/*.{ts,css}'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "D": "1.0.0",
    "core-js": "2.5.7",
    "puppeteer": "1.11.0",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular/cli": "7.1.0",
    "@angular/compiler-cli": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@types/jasmine": "3.3.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.11",
    "codelyzer": "4.5.0",
    "husky": "1.2.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "protractor": "5.4.1",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.1",
    "webpack-bundle-analyzer": "3.0.3"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-prepush": "npm audit --only=prod && run-s lint prettier-test test:ci"
    }
  }
}
